<%for(var i=0; i < users.length; i++){%>
<tr>
    <td><%= i+1%></td>
    <td><%= users[i].Name%></td>
    <td><%= users[i].Age%></td>
    <td><%= users[i].Team%></td>
    <td><%= users[i].Position%></td>
    <td><%= users[i].Contract%></td>
    <td>
        <a href="/update-user" class="btn border-shadow update">
            <span class="text-gradient"><i class="fa-solid fa-pencil"></i></span>
        </a>
        <a class="btn border-shadow delete">
            <span class="text-gradient"><i class="fa-sharp fa-solid fa-delete-left"></i></span>
        </a>
    </td>
</tr>
<% } %>